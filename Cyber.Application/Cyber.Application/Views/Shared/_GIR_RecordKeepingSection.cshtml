            <tr>
                <td width="10%" class="v-al-m text-center">
                    10
                </td>
                <td width="40%" class="v-al-m">
                    <p class="m-0 m-l-5">GMDSS logbook</p>
                </td>
                <td width="50%">
                    <textarea class="form-control col-md-12" name="GMDSSLogbook" rows="1">@Model.GMDSSLogbook</textarea>
                </td>
            </tr>
            <tr>
                <td width="10%" class="v-al-m text-center">
                    11
                </td>
                <td width="40%" class="v-al-m">
                    <p class="m-0 m-l-5">Deck</p>
                </td>
                <td width="50%">
                    <textarea class="form-control col-md-12" name="DeckLogbook5D" rows="1">@Model.DeckLogbook5D</textarea>
                </td>
            </tr>
            <tr>
                <td width="10%" class="v-al-m text-center">
                    12
                </td>
                <td width="40%" class="v-al-m">
