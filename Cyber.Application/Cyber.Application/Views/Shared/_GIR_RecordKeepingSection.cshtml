                <td width="50%">
                    <textarea class="form-control col-md-12" name="PEER" rows="1">@Model.PEER</textarea>
                </td>
            </tr>
            <tr>
                <td colspan="4" width="100%" class="v-al-m">
                    Comments:
                </td>
            </tr>
            <tr>
                <td colspan="4" width="100%">
                    <textarea class="form-control col-md-12" name="RecordKeepingComments" rows="2">@Model.RecordKeepingComments</textarea>
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="Section breakDivPage">
    <div class="SectionHead">
        <u>Section 5E</u> - Last Port State Control and / or Flag State Inspection
    </div>
    <div class="SectionContent">
        <table class="table tblSection">
            <tr>
                <td>
                    <textarea class="form-control col-md-12" name="LastPortStateControl" rows="2">@Model.LastPortStateControl</textarea>
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="SectionComplete">
    <div class="checkbox">
        <label>Section Complete?</label>
        <input type="checkbox" name="IsRecordKeepingSectionComplete" style="margin-left:20px;">
    </div>
</div>
