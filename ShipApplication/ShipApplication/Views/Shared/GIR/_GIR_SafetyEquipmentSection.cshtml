<div class="SectionComplete">
    <div class="checkbox">
        <label>Section Complete?</label>
        @* RDBJ 10/19/2021 Set Name, Value and wrapped in if*@
        @if (Model.IsSafetyEquipmentSectionComplete == true)
        {
            <input type="checkbox" class="GIRData" name="IsSafetyEquipmentSectionComplete" value="true" style="margin-left:20px;" checked />
        }
        else
        {
            <input type="checkbox" class="GIRData" name="IsSafetyEquipmentSectionComplete" value="false" style="margin-left:20px;" />
        }
    </div>
