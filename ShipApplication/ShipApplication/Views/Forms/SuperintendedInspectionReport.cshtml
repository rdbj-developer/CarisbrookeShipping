    .tblSection {
        background-color: white;
    }

    td.notes_column {
        border: 1px solid #ffffff !important;
        background-color: white !important;
    }

    i.material-icons, .glyphicon {
        background-color: #FFFFE1;
        color: green;
        font-size: 20px;
        border: 1px solid black;
        padding: 2px;
        margin-top: 7px;
        cursor: pointer;
    }

