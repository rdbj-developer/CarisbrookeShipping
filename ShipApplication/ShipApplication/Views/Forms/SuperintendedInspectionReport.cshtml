        border: none;
    }

    /* RDBJ 02/19/2022 */
    .popover {
        width: 300px;
        height: 230px;
    }

    .textarea {
        height: 100px;
        border: 1px solid grey;
        border-radius: 1px;
        border-radius: 3px;
        padding: 0px 5px;
        overflow: auto;
        cursor: text;
    }

        .textarea:focus-visible {
            outline: none !important;
        }

    .active:focus {
        outline: none !important;
    }

    .NavButton {
        text-overflow: ellipsis;
        white-space: nowrap;
        overflow: hidden;
    }
