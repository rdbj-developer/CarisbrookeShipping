
<div class="Section breakDiv">
    <div class="SectionHead">
        <u>Section 5B</u> - List of Publications Held On-board
    </div>
    <div class="SectionContent">
        <table class="table tblSection tblSectionManning4d">
            <tr>
                <td width="80%">
                    <p class="m-5">
                        All items listed in the current on board version of Fleet Standing Order TSO 02 - Training and
                        Publications Library and inventory carried out by the DOOW on board within last 3 months.
                    </p>
                </td>
                <td width="20%" class="v-al-m text-center">
                    <label class="radio-inline">
                        @* RDBJ 10/19/2021 Set Name, Value and wrapped in if*@
                        @if (Convert.ToBoolean(Model.CarriedOutByTheDOOW) == true)
                        {
                            <input type="radio" name="CarriedOutByTheDOOW" class="GIRData" value="true" checked><span>Yes</span>
                        }
                        else
                        {
                            <input type="radio" name="CarriedOutByTheDOOW" class="GIRData" value="true"><span>Yes</span>
                        }
                    </label>
                    <label class="radio-inline">
                        @* RDBJ 10/19/2021 Set Name, Value and wrapped in if*@
                        @if (Convert.ToBoolean(Model.CarriedOutByTheDOOW) == false)
                        {
                            <input type="radio" name="CarriedOutByTheDOOW" class="GIRData" value="false" checked><span>No</span>
                        }
                        else
                        {
                            <input type="radio" name="CarriedOutByTheDOOW" class="GIRData" value="false"><span>No</span>
                        }
                    </label>
                </td>
            </tr>
        </table>
    </div>
</div>
