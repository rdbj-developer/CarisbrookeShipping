
<div class="Section breakDiv clsSection">
    <div class="SectionHead">
        <u>Section 5A</u> - Certification
    </div>
    <div class="SectionContent">
        <table class="table tblSection tblSectionManning4d">
            <tr>
                <td width="100%"><p class="m-5">All ships certificates checked against latest Ship Certificate Status Index (SCSI):</p></td>
            </tr>
            <tr class="addDeficiencies">
                <td width="100%" class="clsDeficiencies"><textarea class="form-control col-md-12 GIRData" name="CertificationIndex">@(Model != null ? Model.CertificationIndex : "")</textarea> </td>
            </tr>
        </table>
    </div>
</div>
<div class="SectionComplete">
    <div class="checkbox">
        <label>Section Complete?</label>
        @* RDBJ 10/19/2021 Set Name and Value *@
        <input type="checkbox" name="IsShipCertificationSectionComplete" class="GIRData" style="margin-left:20px;" value="true">
    </div>
</div>



