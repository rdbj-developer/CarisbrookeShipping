<div class="SectionComplete">
    <div class="checkbox">
        <label>Section Complete?</label>
        @* RDBJ 10/20/2021 Set Name, Value and wrapped in if*@
        @if (Model.IsDeckSectionComplete == true)
        {
            <input type="checkbox" class="GIRData" name="IsDeckSectionComplete" value="true" style="margin-left:20px;" checked />
        }
        else
        {
            <input type="checkbox" class="GIRData" name="IsDeckSectionComplete" value="false" style="margin-left:20px;" />
        }
    </div>
</div>
