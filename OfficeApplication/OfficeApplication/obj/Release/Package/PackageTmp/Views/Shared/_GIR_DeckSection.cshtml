<div class="Section breakDivPage clsSection">
    <div class="SectionHead">
        <u>Section 5O</u> - Boarding Equipment
    </div>
    <div class="SectionContent">
        <table class="table tblSection">
            <tr>
                <td width="10%" class="v-al-m text-center">
                    No.
                </td>
                <td width="40%" class="v-al-m text-center">Item</td>
                <td width="50%" class="v-al-m text-center">Comment</td>
            </tr>
            <tr class="addDeficiencies">
                <td width="10%" class="v-al-m text-center clsItemNo">
                    1
                </td>
                <td width="40%" class="v-al-m">
                    <p class="m-0 m-l-5">Portable gangway</p>
                </td>
                <td width="50%" class="clsDeficiencies">
                    <textarea class="form-control col-md-12 GIRData" name="PortablegangwayComment" rows="1">@Model.PortablegangwayComment</textarea>
                </td>
            </tr>
            <tr class="addDeficiencies">
                <td width="10%" class="v-al-m text-center clsItemNo">
                    2
                </td>
                <td width="40%" class="v-al-m">
                    <p class="m-0 m-l-5">Safety net</p>
                </td>
                <td width="50%" class="clsDeficiencies">
                    <textarea class="form-control col-md-12 GIRData" name="SafetynetComment" rows="1">@Model.SafetynetComment</textarea>
                </td>
            </tr>
            <tr class="addDeficiencies">
                <td width="10%" class="v-al-m text-center clsItemNo">
                    3
                </td>
                <td width="40%" class="v-al-m">
                    <p class="m-0 m-l-5">Accommodation Ladder(s)</p>
                </td>
                <td width="50%" class="clsDeficiencies">
                    <textarea class="form-control col-md-12 GIRData" name="AccommodationLadderComment" rows="1">@Model.AccommodationLadderComment</textarea>
                </td>
            </tr>
            <tr class="addDeficiencies">
                <td width="10%" class="v-al-m text-center clsItemNo">
                    4
                </td>
                <td width="40%" class="v-al-m">
                    <p class="m-0 m-l-5">Safe access provided</p>
                </td>
                <td width="50%" class="clsDeficiencies">
                    <textarea class="form-control col-md-12 GIRData" name="SafeaccessprovidedComment" rows="1">@Model.SafeaccessprovidedComment</textarea>
                </td>
            </tr>
            <tr class="addDeficiencies">
                <td width="10%" class="v-al-m text-center clsItemNo">
                    5
                </td>
                <td width="40%" class="v-al-m">
                    <p class="m-0 m-l-5">Pilot ladders</p>
                </td>
                <td width="50%" class="clsDeficiencies">
                    <textarea class="form-control col-md-12 GIRData" name="PilotladdersComment" rows="1">@Model.PilotladdersComment</textarea>
                </td>
            </tr>
            <tr>
                <td colspan="4" width="100%" class="v-al-m">
                    Comments:
                </td>
            </tr>
            <tr class="addDeficiencies">
                <td colspan="3" width="100%" class="clsDeficiencies">
                    <textarea class="form-control col-md-12 GIRData" name="BoardingEquipmentComment" rows="2">@Model.BoardingEquipmentComment</textarea>
                </td>
            </tr>
        </table>
    </div>
</div>
