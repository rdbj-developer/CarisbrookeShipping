    function getBackGroundClass(objDiff) {
        var strClass = '';
        if (objDiff.months >= 3 && objDiff.months < 6) {
            strClass = "bg-light-green";
        } else if (objDiff.months != 0 && (objDiff.months > 0 && objDiff.months < 3)) {
            strClass = "bg-yellow-active";
        } else if (objDiff.months != 0 && objDiff.months >= 12) {
            strClass = "bg-red";    //red colour for overdue.
        }
        return strClass;
    }
