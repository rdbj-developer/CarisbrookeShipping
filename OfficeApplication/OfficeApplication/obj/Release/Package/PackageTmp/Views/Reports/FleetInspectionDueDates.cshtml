    function difference(d1) {
        var d2 = new Date();
        var m = moment(d1);

        var years = m.diff(d2, 'years');
        m.add(-years, 'years');
        var months = m.diff(d2, 'months');
        m.add(-months, 'months');
        var days = m.diff(d2, 'days');

        // JSL 01/18/2023
        m.add(-days, 'days');

        var noOfWeeks = moment(m._i, 'DD-MM-YYYY').diff(moment(d2, 'DD-MM-YYYY'), 'week');
        var retObjDiff = {
            years: years,
            months: months,
            days: days,
            weeks: noOfWeeks
        }
        // End JSL 01/18/2023

        return retObjDiff;  // JSL 01/18/2023 wrapped in object
    }

    // JSL 01/19/2023
    function getDateDifference(givenDate, isNeedToUseEndOfCurrentYearDate = false, isSetCurrDateHigher = false) {
        var curDate = new Date();
        var momGivenDate = moment(givenDate);

        if (isSetCurrDateHigher) {
            curDate = new Date(curDate.setMonth(curDate.getMonth() + 6));
        }

        if (isNeedToUseEndOfCurrentYearDate) {
            curDate = moment(givenDate);
            momGivenDate = moment(new Date());
            /*var date = new Date();
            momGivenDate = moment(new Date(date.setMonth(date.getMonth() + 6)));*/
        }

        var years = momGivenDate.diff(curDate, 'years');
        //momGivenDate.add(-years, 'years');
        var months = momGivenDate.diff(curDate, 'months');
        //momGivenDate.add(-months, 'months');
        var days = momGivenDate.diff(curDate, 'days');
        //momGivenDate.add(-days, 'days');

        var noOfWeeks = moment(momGivenDate._i, 'DD-MM-YYYY').diff(moment(curDate, 'DD-MM-YYYY'), 'week');
        var retObjDiff = {
            years: years,
            months: months,
            days: days,
            weeks: noOfWeeks
        }

        return retObjDiff;  
    }
