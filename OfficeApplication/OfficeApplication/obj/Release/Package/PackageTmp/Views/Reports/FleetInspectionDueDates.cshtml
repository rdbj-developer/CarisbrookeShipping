    // JSL 01/20/2023
    function checkSIDue(dueDate, strDate, fieldName, SMCIsVerified, ShipId) {
        var strTemplate = '<div style="text-align:center;" {clickEvent}>';
        strTemplate += '<span class="{class} k-padding">'
        var strClass = "", striClickEvent = "";
        if (dueDate && dueDate != null && dueDate != undefined) {
            var IsNeedToUseCalendarYear = false;    // JSL 01/19/2023
            var currentDate = new Date();
            var givenDate = new Date(dueDate);

            // JSL 10/08/2022
            if (fieldName == "NextSIDue"
                //|| fieldName == "LastGI"  // JSL 01/19/2023 commented
            ) {
                givenDate = new Date(givenDate.setMonth(givenDate.getMonth() + 12));
            }
            // End JSL 10/08/2022

            var objDiff = difference(givenDate);
            if (objDiff) {
                if (fieldName == "LastFlagGI" || fieldName == "LastFlagASI" || fieldName == "LastFlagCICA") {
                    if (givenDate > currentDate) {
                        //if (objDiff.years == 0 && objDiff.months > 1 && objDiff.months <= 6) {    // JSL 10/01/2022 commented this line //green for due within 6 months
                        //if (objDiff.years == 0 && objDiff.months > 3 && objDiff.months <= 6) {  // JSL 01/18/2023 commented  // JSL 10/01/2022 //green for due within 6 and 3 months
                        if (objDiff.years == 0 && objDiff.months > 3 && objDiff.months <= 5) {  // JSL 01/18/2023   //green for due within 6 and 3 months
                            strClass = "bg-light-green";
                        }
                        else if (objDiff.years == 0
                            //&& objDiff.months > 0 // RDBJ 03/31/2022 Commented this line
                            //&& objDiff.months == 0  // JSL 01/18/2023 commented  // RDBJ 03/31/2022
                            && objDiff.months >= 0  // JSL 01/18/2023
                            //&& objDiff.months <= 1) { // JSL 10/01/2022 commented this line // orange for due within 1 month
                            //&& objDiff.months <= 3) {   // JSL 01/18/2023 commented   // JSL 10/01/2022 // orange for due within 3 months
                            && objDiff.months <= 2) {   // JSL 01/18/2023   // orange for due within 3 months
                            strClass = "bg-yellow-active";
                        }
                    }
                    else {//red colour for overdue.
                        strClass = "bg-red";
                    }

                }

                if (fieldName == "NextSIDue") {
                    // JSL 01/18/2023 commented
                    /*
                    if (givenDate > currentDate) {
                        // JSL 10/03/2022
                        //if (objDiff.years == 0 && objDiff.months > 3 && objDiff.months <= 6) {  // JSL 01/18/2023 commented 
                        if (objDiff.years == 0 && objDiff.months > 3 && objDiff.months <= 5) {  // JSL 01/18/2023 
                            strClass = "bg-light-green";
                        }
                        // End JSL 10/03/2022
                        else if (objDiff.years == 0
                            //&& objDiff.months <= 0    // RDBJ 03/31/2022
                            && objDiff.months == 0  // JSL 10/03/2022
                            //&& objDiff.months <= 3  // JSL 01/18/2023 commented   // RDBJ 03/31/2022
                            && objDiff.months <= 2  // JSL 01/18/2023
                        ) {
                            strClass = "bg-yellow-active";
                        }
                    } else {
                        strClass = "bg-red";//red colour for overdue.
                    }
                    */
                    // End JSL 01/18/2023 commented
                }

                if (fieldName == "LastFSTO") {
                    if (objDiff.years < 0) {
                        //strClass = "bg-yellow-active";    // JSL 01/18/2023 commented due to no need colour
                    }
                }

                if (fieldName == "LastSI"    
                    || fieldName == "LastGI"
                ) {
                    // JSL 01/19/2023
                    var lastDateOfCurrentYear = moment(new Date()).endOf('year');

                    if (givenDate < currentDate) {
                        objDiff = getDateDifference(givenDate, true);
                        var objDiffFromCurrent;
                        var totalMonths;    // JSL 01/20/2023
                        if (objDiff.months >= 12) {
                            objDiffFromCurrent = difference(givenDate);
                        }
                        else {
                            objDiffFromCurrent = getDateDifference(lastDateOfCurrentYear, true);
                        }
                        if (objDiffFromCurrent.months < 0) {
                            totalMonths = parseInt(objDiff.months) + parseInt(objDiffFromCurrent.months);
                        }
                        objDiff.months = totalMonths;

                        strClass = getBackGroundClass(objDiff);
                    }
                    // End JSL 01/19/2023
                }

                // JSL 01/18/2023
                if (fieldName == "InternalAuditDue") {
                    if (givenDate > currentDate) {
                        /*var momGivenDate = moment(givenDate, 'DD-MM-YYYY');
                        var momCurrnDate = moment(currentDate, 'DD-MM-YYYY');
                        var noOfWeeks = momGivenDate.diff(momCurrnDate, 'week');*/

                        if (objDiff.weeks >= 6 && objDiff.weeks < 12) { // 12 weeks before due date.
                            strClass = "bg-light-green";
                        } else if (objDiff.weeks >= 0 && objDiff.weeks < 6) {   // 6 weeks before due date.
                            strClass = "bg-yellow-active";
                        }
                    } else {
                        strClass = "bg-red";    //red colour for overdue.
                    }
                }
                // End JSL 01/18/2023

