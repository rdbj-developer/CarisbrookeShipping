                    if ($(this).hasClass("update-confirm")) {
                        var date = $input.datepicker('getFormattedDate');
                        $input.datepicker('hide');

                        UpdateFleetInspectionDueDates(obj, date, strClass, ShipId, fieldName); // RDBJ 12/24/2021

