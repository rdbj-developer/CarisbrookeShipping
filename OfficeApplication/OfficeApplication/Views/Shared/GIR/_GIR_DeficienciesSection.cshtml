    //RDBJ 11/02/2021
    function deleteFile(ctr, value) {
        $(ctr).parent().remove();

        $.each(girDeficienciesFiles.Files, function (i) {
            if (girDeficienciesFiles.Files[i].FileName === value) {
                girDeficienciesFiles.Files.splice(i, 1);
                return false;
            }
        });

        fileData.delete(value); // JSL 12/02/2022
    }
